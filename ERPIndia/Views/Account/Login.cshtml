            rating.forEach(r => r.classList.remove("rating_active"));
            clicked.classList.add("rating_active");

            const data = clicked.dataset.rate;
            const rateEmoji = clicked.textContent.split(" ")[0];
            const rateText = clicked.textContent.split(" ")[1];
            console.log(rateEmoji);

            noticeContainer.innerHTML = "";
            const html = `<section class="feedback_notice">
                                                                                    <p> ${data < 3 ?
                    `Sorry to heard that ${rateEmoji}! Thank you for the feedback ` :
                    `${rateText}! Thank you ${rateEmoji} `
                } </p>


                                                                                </section>`;

            noticeContainer.insertAdjacentHTML("afterbegin", html);
        });
        //# sourceURL=pen.js
    </script>
    <script>
    $(document).ready(function () {
        // Store original button text and state
        const $submitBtn = $('#frmLogin button[type="submit"], #frmLogin input[type="submit"]');
        const originalBtnText = $submitBtn.text() || $submitBtn.val() || 'Login';

        // Function to set loading state
        function setLoadingState() {
            $submitBtn.prop('disabled', true);
            if ($submitBtn.is('button')) {
                $submitBtn.html('<i class="fas fa-spinner fa-spin"></i> Logging in...');
            } else {
                $submitBtn.val('Logging in...');
            }
        }

        // Function to reset button to default state
        function resetButtonState() {
            $submitBtn.prop('disabled', false);
            if ($submitBtn.is('button')) {
                $submitBtn.html(originalBtnText);
            } else {
                $submitBtn.val(originalBtnText);
            }
        }

        @if (!string.IsNullOrEmpty(ViewBag.ErrorMsg))
        {
            <text>
            // Display server error message and reset button
            $('#error-messages').empty();
            $('#error-messages').append('<li>Invalid Login Details</li>');
            $('#error-box').fadeIn();

            // Reset button state since login failed
            resetButtonState();

            // Auto-hide after 3 seconds
            setTimeout(function () {
                $('#error-box').fadeOut();
            }, 3000);
            </text>
        }

        $('#frmLogin').on('submit', function (e) {
            let errors = [];
            let username = $('#UserName').val().trim();
            let password = $('#Password').val().trim();
            let code = $('#Code').val().trim();

            // Validation
            if (!username) errors.push("Username is required.");
            if (!password) errors.push("Password is required.");
            if (!code) errors.push("Client Code is required.");

            if (errors.length > 0) {
                // Prevent submission and show errors
                e.preventDefault();
                $('#error-messages').empty();
                errors.forEach(function (msg) {
                    $('#error-messages').append(`<li>${msg}</li>`);
                });
                $('#error-box').fadeIn();

                // Reset button state since validation failed
                resetButtonState();

                setTimeout(function () {
                    $('#error-box').fadeOut();
                }, 3000);
            } else {
                // Validation passed - show loading state
                setLoadingState();

                // Let the form submit naturally
                // The loading state will be reset by server response or failsafe
            }
        });

        // Failsafe: Reset button after 30 seconds if still loading
        setTimeout(function() {
            if ($submitBtn.prop('disabled')) {
                resetButtonState();
                // Optionally show timeout message
                $('#error-messages').empty();
                $('#error-messages').append('<li>Request timeout. Please try again.</li>');
                $('#error-box').fadeIn();
                setTimeout(function () {
                    $('#error-box').fadeOut();
                }, 3000);
            }
        }, 30000);

        // Reset button if user navigates back to page
        $(window).on('pageshow', function(e) {
            if (e.originalEvent.persisted) {
                resetButtonState();
            }
        });
    });
    </script>



