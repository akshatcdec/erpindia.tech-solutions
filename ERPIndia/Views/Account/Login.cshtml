        }

        .container-login100-form-btn {
            display: -webkit-box;
            display: -webkit-flex;
            display: -moz-box;
            display: -ms-flexbox;
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            margin-bottom: 25px; /* Added space between login button and Need Help */
        }

        .wrap-login100-form-btn {
            width: 100%;
            display: flex;
            justify-content: center;
            align-items: center;
            background: linear-gradient(to right, #00dbde, #fc00ff);
            border-radius: 50px;
            padding: 3px;
            box-shadow: 0 4px 15px rgba(0, 219, 222, 0.4);
            position: relative;
            overflow: hidden;
            transition: all 0.3s ease;
        }

            .wrap-login100-form-btn:focus {
                outline: none !important;
            }

            .wrap-login100-form-btn:focus-within {
                outline: none !important;
            }

            .wrap-login100-form-btn::before {
                content: '';
                position: absolute;
                top: 0;
                left: -100%;
                width: 100%;
                height: 100%;
                background: linear-gradient(to right, transparent, rgba(255,255,255,0.3), transparent);
                transition: left 0.6s ease;
            }

            .wrap-login100-form-btn:hover::before {
                left: 100%;
            }

            .wrap-login100-form-btn:hover {
                box-shadow: 0 6px 20px rgba(0, 219, 222, 0.6);
                transform: translateY(-2px);
            }

        .error-box {
            position: fixed;
            top: 30px;
            right: 30px;
            background-color: #fff5f5;
            padding: 15px 20px;
            border-radius: 12px;
            box-shadow: 0 4px 12px rgba(255, 0, 0, 0.2);
            display: flex;
            align-items: flex-start;
            max-width: 350px;
            font-family: 'Poppins', sans-serif;
            font-size: 14px;
            color: #a94442;
            z-index: 9999;
            border-left: 5px solid #ff4d4f;
        }

        .error-icon {
            font-size: 20px;
            color: #ff4d4f;
            margin-right: 12px;
            margin-top: 4px;
        }

        .error-list li {
            padding: 4px 0;
            font-size: 15px;
        }

        .error-field {
            font-weight: 600;
            color: #d8000c;
            margin-right: 5px;
        }

        /* Loading Overlay Styles */
        .loading-overlay {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.8);
            display: none;
            justify-content: center;
            align-items: center;
            z-index: 99999;
        }

        .loading-content {
            text-align: center;
            color: white;
        }

        .loading-spinner {
            width: 60px;
            height: 60px;
            border: 4px solid rgba(255, 255, 255, 0.3);
            border-top: 4px solid #00dbde;
            border-radius: 50%;
            animation: spin 1s linear infinite;
            margin: 0 auto 20px;
        }

        @@keyframes spin {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }

        .loading-text {
            font-size: 18px;
            font-weight: bold;
            margin-bottom: 10px;
        }

        .loading-subtext {
            font-size: 14px;
            opacity: 0.8;
        }

        /* Button Loading State */
        .login100-form-btn.loading {
            pointer-events: none;
            opacity: 0.8;
        }

        .btn-spinner {
            width: 18px;
            height: 18px;
            border: 2px solid rgba(255, 255, 255, 0.3);
            border-top: 2px solid white;
            border-radius: 50%;
            animation: spin 1s linear infinite;
            display: inline-block;
            margin-right: 8px;
            vertical-align: middle;
        }
