							<xs:element name="D25" type="xs:string" minOccurs="0"/>
							<xs:element name="D26" type="xs:string" minOccurs="0"/>
							<xs:element name="D27" type="xs:string" minOccurs="0"/>
							<xs:element name="D28" type="xs:string" minOccurs="0"/>
							<xs:element name="D29" type="xs:string" minOccurs="0"/>
							<xs:element name="D30" type="xs:string" minOccurs="0"/>
							<xs:element name="D31" type="xs:string" minOccurs="0"/>
							<xs:element name="D32" type="xs:string" minOccurs="0"/>
							<xs:element name="D33" type="xs:string" minOccurs="0"/>
							<xs:element name="D34" type="xs:string" minOccurs="0"/>
							<xs:element name="D35" type="xs:string" minOccurs="0"/>
							<xs:element name="D36" type="xs:string" minOccurs="0"/>
							<xs:element name="D37" type="xs:string" minOccurs="0"/>
							<xs:element name="D38" type="xs:string" minOccurs="0"/>
							<xs:element name="D39" type="xs:string" minOccurs="0"/>
							<xs:element name="D40" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
